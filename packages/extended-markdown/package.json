{
  "name": "@felix/extended-markdown",
  "version": "0.1.0",
  "description": "Extended markdown parser with Mermaid and Excalidraw support",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "react"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felix-ai/felix.git"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "require": "./dist/react.js",
      "import": "./dist/react.mjs"
    }
  },
  "dependencies": {
    "mdast": "^3.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "peerDependencies": {
    "@excalidraw/excalidraw": "^0.17.0",
    "mermaid": "^10.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-markdown": "^9.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "react-markdown": {
      "optional": true
    },
    "mermaid": {
      "optional": true
    },
    "@excalidraw/excalidraw": {
      "optional": true
    }
  },
  "devDependencies": {
    "@excalidraw/excalidraw": "^0.18.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/mdast": "^4.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@babel/core": "^7.26.0",
    "babel-jest": "^29.7.0",
    "@babel/preset-env": "^7.26.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "mermaid": "^11.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "ts-jest": "^29.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "markdown",
    "mermaid",
    "excalidraw",
    "parser",
    "react"
  ],
  "author": "Felix Team",
  "license": "UNLICENSED"
}
