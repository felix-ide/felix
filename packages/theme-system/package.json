{
  "name": "@felix/theme-system",
  "version": "0.2.0",
  "description": "Advanced theme system for AIgent Smith with custom theme support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
